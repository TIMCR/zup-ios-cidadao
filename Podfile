# Uncomment this line to define a global platform for your project
platform :ios, '9.0'

def config_pods
    pod 'SDWebImage', '~>3.7'
    pod 'ISO8601DateFormatter', '~> 0.7'
    pod 'AFNetworking'
    pod 'AFNetworkActivityLogger', '~> 2.0'
    pod 'GoogleMaps'
end

target 'zup' do
    config_pods
end

target 'zupTests' do
    config_pods
end
